@model PreAgendamento
@{
    ViewData["Title"] = "Agendamentos";
}
<body>
        <form method="POST">
    <p>
       <label asp-for="Nome">Nome:</label>
       <input asp-for="Nome" />
    </p>
    <p>
       <label asp-for="Telefone">Telefone:</label>
       <input asp-for="Telefone" />
    </p>
    <p>
       <label asp-for="DataTimes">Data:</label>
       <input asp-for="DataTimes" />
    </p>
    <p>
       <label asp-for="Animal">Grupo do Animal:</label>
           <select asp-for="Animal">
           <option value="Felino">Felino</option>
           <option value="Canino">Canino</option>
           <option value="Ave">Ave</option>
       </select>
    </p>
    <p>
        <p>Necessidade</p>
        <textarea asp-for="Necessidade"></textarea>
    </p>
   <input type="submit" value="Cadastrar" />
</form>            
    </main>
</body>