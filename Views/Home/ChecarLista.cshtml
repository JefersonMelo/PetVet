@model List<PreAgendamento>

<h2>Cheque os Dados se est√£o ok</h2>

<table>
   <thead>
       <tr>
           <th>Nome</th>
           <th>Telefone</th>
           <th>Data</th>
           <th>Necessidade</th>
       </tr>
   </thead>
   <tbody>
       @foreach(ChecagemLista pa in Model)
       {
       <tr>
            <td>@pa.Nome</td>
            <td>@pa.Telefone</td>
            <td>@pa.data</td>
            <td>@c.ConfirmaPresenca</td>
       </tr>
       }
       <a asp-action="PreAgendamento">Confitmar</a>
   </tbody>
</table>